{
  "cross_system_communication": {
    "enabled": "ENABLE_TRUE",
    "default_timeout": "DEFAULT_TIMEOUT",
    "default_retry_attempts": "VALUE_THREE",
    "health_check_interval": "LONG_TIMEOUT",
    "max_connections_per_system": 10,
    "connection_pool_size": 20,
    "message_queue_size": "VALUE_THOUSAND",
    "enable_compression": "ENABLE_TRUE",
    "enable_encryption": "ENABLE_FALSE",
    "log_level": "INFO"
  },
  "protocols": {
    "http": {
      "enabled": "ENABLE_TRUE",
      "default_port": 80,
      "timeout": "DEFAULT_TIMEOUT",
      "max_redirects": 5,
      "enable_keepalive": "ENABLE_TRUE",
      "keepalive_timeout": "LONG_TIMEOUT"
    },
    "https": {
      "enabled": "ENABLE_TRUE",
      "default_port": 443,
      "timeout": "DEFAULT_TIMEOUT",
      "verify_ssl": "ENABLE_TRUE",
      "enable_keepalive": "ENABLE_TRUE",
      "keepalive_timeout": "LONG_TIMEOUT"
    },
    "websocket": {
      "enabled": "ENABLE_TRUE",
      "default_port": 8080,
      "ping_interval": "DEFAULT_TIMEOUT",
      "ping_timeout": "SHORT_TIMEOUT",
      "max_message_size": 1048576,
      "enable_compression": "ENABLE_TRUE"
    },
    "tcp": {
      "enabled": "ENABLE_TRUE",
      "default_port": 8000,
      "timeout": "DEFAULT_TIMEOUT",
      "buffer_size": 8192,
      "enable_nagle": "ENABLE_FALSE",
      "keepalive": "ENABLE_TRUE"
    },
    "udp": {
      "enabled": "ENABLE_FALSE",
      "default_port": 8001,
      "timeout": "SHORT_TIMEOUT",
      "buffer_size": 65507,
      "enable_broadcast": "ENABLE_FALSE"
    }
  },
  "systems": {
    "agent_cellphone_v2": {
      "system_id": "agent_cellphone_v2",
      "name": "Agent Cellphone V2 Core System",
      "protocol": "http",
      "host": "localhost",
      "port": 8000,
      "path": "/api/v2",
      "timeout": "DEFAULT_TIMEOUT",
      "retry_attempts": "VALUE_THREE",
      "health_check_interval": "DEFAULT_TIMEOUT",
      "credentials": null,
      "metadata": {
        "version": "2.0.0",
        "capabilities": [
          "api_management",
          "middleware_orchestration",
          "service_registry"
        ],
        "environment": "development"
      }
    },
    "external_api_gateway": {
      "system_id": "external_api_gateway",
      "name": "External API Gateway",
      "protocol": "https",
      "host": "api.example.com",
      "port": 443,
      "path": "/v1",
      "timeout": "LONG_TIMEOUT",
      "retry_attempts": 5,
      "health_check_interval": 120.0,
      "credentials": {
        "api_key": "${EXTERNAL_API_KEY}",
        "api_secret": "${EXTERNAL_API_SECRET}"
      },
      "metadata": {
        "version": "1.0.0",
        "capabilities": [
          "external_integration",
          "rate_limiting",
          "authentication"
        ],
        "environment": "production"
      }
    },
    "database_system": {
      "system_id": "database_system",
      "name": "Database Management System",
      "protocol": "tcp",
      "host": "localhost",
      "port": 5432,
      "path": "",
      "timeout": 15.0,
      "retry_attempts": "VALUE_THREE",
      "health_check_interval": "LONG_TIMEOUT",
      "credentials": {
        "username": "${DB_USERNAME}",
        "password": "${DB_PASSWORD}"
      },
      "metadata": {
        "version": "13.0",
        "capabilities": [
          "data_storage",
          "query_processing",
          "transaction_management"
        ],
        "environment": "development"
      }
    },
    "message_broker": {
      "system_id": "message_broker",
      "name": "Message Broker System",
      "protocol": "websocket",
      "host": "localhost",
      "port": 8080,
      "path": "/ws",
      "timeout": "DEFAULT_TIMEOUT",
      "retry_attempts": "VALUE_THREE",
      "health_check_interval": "DEFAULT_TIMEOUT",
      "credentials": null,
      "metadata": {
        "version": "5.0.0",
        "capabilities": [
          "message_queuing",
          "pub_sub",
          "task_distribution"
        ],
        "environment": "development"
      }
    },
    "monitoring_system": {
      "system_id": "monitoring_system",
      "name": "System Monitoring and Metrics",
      "protocol": "http",
      "host": "localhost",
      "port": 9090,
      "path": "/metrics",
      "timeout": "SHORT_TIMEOUT",
      "retry_attempts": "VALUE_TWO",
      "health_check_interval": 15.0,
      "credentials": null,
      "metadata": {
        "version": "2.0.0",
        "capabilities": [
          "metrics_collection",
          "health_monitoring",
          "alerting"
        ],
        "environment": "development"
      }
    }
  },
  "integration_testing": {
    "enabled": "ENABLE_TRUE",
    "test_timeout": "CRITICAL_TIMEOUT",
    "parallel_execution": "ENABLE_TRUE",
    "max_parallel_tests": 5,
    "stop_on_failure": "ENABLE_FALSE",
    "retry_failed_tests": "ENABLE_TRUE",
    "max_retries": "VALUE_THREE",
    "generate_reports": "ENABLE_TRUE",
    "report_format": "json",
    "log_level": "INFO",
    "test_suites": {
      "cross_system_communication": {
        "enabled": "ENABLE_TRUE",
        "priority": "high",
        "timeout": 600.0,
        "tests": [
          "test_system_connections",
          "test_message_routing",
          "test_protocol_handling",
          "test_error_handling",
          "test_health_checks"
        ]
      },
      "api_integration": {
        "enabled": "ENABLE_TRUE",
        "priority": "high",
        "timeout": "CRITICAL_TIMEOUT",
        "tests": [
          "test_endpoint_registration",
          "test_request_handling",
          "test_response_validation",
          "test_error_scenarios",
          "test_rate_limiting"
        ]
      },
      "middleware_integration": {
        "enabled": "ENABLE_TRUE",
        "priority": "normal",
        "timeout": "CRITICAL_TIMEOUT",
        "tests": [
          "test_middleware_chains",
          "test_data_transformation",
          "test_validation_rules",
          "test_routing_logic",
          "test_performance_metrics"
        ]
      },
      "end_to_end_workflows": {
        "enabled": "ENABLE_TRUE",
        "priority": "critical",
        "timeout": 900.0,
        "tests": [
          "test_complete_user_workflow",
          "test_system_failover",
          "test_data_consistency",
          "test_performance_under_load",
          "test_security_scenarios"
        ]
      }
    }
  },
  "message_routing": {
    "default_routing": "direct",
    "routing_rules": [
      {
        "pattern": "system.*.health",
        "target": "monitoring_system",
        "priority": "high",
        "timeout": "SHORT_TIMEOUT"
      },
      {
        "pattern": "api.*.request",
        "target": "external_api_gateway",
        "priority": "normal",
        "timeout": "DEFAULT_TIMEOUT"
      },
      {
        "pattern": "data.*.query",
        "target": "database_system",
        "priority": "normal",
        "timeout": 15.0
      },
      {
        "pattern": "task.*.distribute",
        "target": "message_broker",
        "priority": "high",
        "timeout": 20.0
      }
    ],
    "fallback_routing": {
      "default_target": "agent_cellphone_v2",
      "retry_attempts": "VALUE_TWO",
      "timeout": "LONG_TIMEOUT"
    }
  },
  "security": {
    "authentication": {
      "enabled": "ENABLE_TRUE",
      "method": "api_key",
      "api_key_header": "X-API-Key",
      "api_key_env_var": "CROSS_SYSTEM_API_KEY"
    },
    "authorization": {
      "enabled": "ENABLE_TRUE",
      "role_based_access": "ENABLE_TRUE",
      "default_role": "readonly"
    },
    "encryption": {
      "enabled": "ENABLE_FALSE",
      "algorithm": "AES-256",
      "key_rotation_interval": 86400
    },
    "rate_limiting": {
      "enabled": "ENABLE_TRUE",
      "requests_per_minute": "VALUE_HUNDRED",
      "burst_limit": 20,
      "per_system_limits": "ENABLE_TRUE"
    }
  },
  "monitoring": {
    "metrics_collection": {
      "enabled": "ENABLE_TRUE",
      "collection_interval": 15.0,
      "retention_period": 86400
    },
    "health_checks": {
      "enabled": "ENABLE_TRUE",
      "check_interval": "DEFAULT_TIMEOUT",
      "timeout": "SHORT_TIMEOUT",
      "failure_threshold": "VALUE_THREE"
    },
    "alerting": {
      "enabled": "ENABLE_TRUE",
      "alert_channels": [
        "log",
        "email"
      ],
      "critical_threshold": 0.8,
      "warning_threshold": 0.9
    },
    "logging": {
      "level": "INFO",
      "format": "json",
      "output": "file",
      "file_path": "logs/cross_system_communication.log",
      "max_file_size": "10MB",
      "backup_count": 5
    }
  },
  "performance": {
    "connection_pooling": {
      "enabled": "ENABLE_TRUE",
      "max_connections": "VALUE_HUNDRED",
      "min_connections": 10,
      "connection_timeout": "DEFAULT_TIMEOUT",
      "idle_timeout": "CRITICAL_TIMEOUT"
    },
    "caching": {
      "enabled": "ENABLE_TRUE",
      "cache_type": "memory",
      "max_size": "VALUE_THOUSAND",
      "ttl": "CRITICAL_TIMEOUT"
    },
    "compression": {
      "enabled": "ENABLE_TRUE",
      "algorithm": "gzip",
      "min_size": 1024,
      "compression_level": 6
    },
    "load_balancing": {
      "enabled": "ENABLE_FALSE",
      "algorithm": "round_robin",
      "health_check_weight": 0.3
    }
  },
  "deployment": {
    "environment": "development",
    "auto_restart": "ENABLE_TRUE",
    "graceful_shutdown": "ENABLE_TRUE",
    "shutdown_timeout": "DEFAULT_TIMEOUT",
    "health_check_endpoint": "/health",
    "readiness_probe": "/ready",
    "liveness_probe": "/live",
    "resource_limits": {
      "max_memory": "512MB",
      "max_cpu": "2.0",
      "max_connections": "VALUE_THOUSAND"
    }
  }
}