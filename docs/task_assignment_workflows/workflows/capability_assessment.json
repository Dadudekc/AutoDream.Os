{
  "workflow_id": "capability_assessment_001",
  "category": "capability_assessment",
  "name": "Agent Capability Assessment and Matching",
  "description": "Workflow for evaluating agent capabilities and matching tasks to agents",
  "priority": "NORMAL",
  "steps": [
    {
      "step_id": "load_agent_capabilities",
      "action": "read_capability_config",
      "inputs": {
        "config_path": "config/agent_capabilities.json"
      },
      "outputs": ["agent_capabilities"],
      "dependencies": [],
      "timeout_seconds": 30,
      "retry_count": 2
    },
    {
      "step_id": "evaluate_agent_skills",
      "action": "assess_skills_for_task",
      "inputs": {
        "task": "current_task",
        "agents": "agent_capabilities"
      },
      "outputs": ["skill_matches"],
      "dependencies": ["load_agent_capabilities"],
      "timeout_seconds": 45,
      "retry_count": 1
    },
    {
      "step_id": "check_workload",
      "action": "analyze_agent_workload",
      "inputs": {
        "agents": "skill_matches"
      },
      "outputs": ["workload_analysis"],
      "dependencies": ["evaluate_agent_skills"],
      "timeout_seconds": 30,
      "retry_count": 1
    },
    {
      "step_id": "match_task_to_agent",
      "action": "select_best_match",
      "inputs": {
        "task": "current_task",
        "matches": "skill_matches",
        "workload": "workload_analysis"
      },
      "outputs": ["selected_agent"],
      "dependencies": ["check_workload"],
      "timeout_seconds": 30,
      "retry_count": 1
    }
  ],
  "validation_rules": [
    {
      "rule_id": "agent_available",
      "condition": "selected_agent.status == 'ACTIVE'",
      "error_message": "Selected agent not available"
    },
    {
      "rule_id": "has_required_skills",
      "condition": "selected_agent.capabilities.contains(task.required_skills)",
      "error_message": "Agent missing required skills"
    }
  ],
  "metadata": {
    "created_by": "Agent-5",
    "created_at": "2025-10-01T12:15:00Z",
    "version": "1.0.0",
    "tags": ["agent_matching", "capability_assessment", "v2_compliant"]
  }
}
