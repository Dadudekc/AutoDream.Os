{
    "mcpServers": {
      "orchestrator": {
        "command": "python",
        "args": ["-m", "src.core.orchestrator"],
        "env": {
          "APP_ENV": "${APP_ENV}",
          "ENVIRONMENT": "${APP_ENV}",
          "LOG_LEVEL": "${LOG_LEVEL}",
          "ENABLE_DEVELOPMENT_FEATURES": "${ENABLE_DEVELOPMENT_FEATURES}",
          "ENABLE_TESTING_MODE": "${ENABLE_TESTING_MODE}",
          "VERBOSE_LOGGING": "${VERBOSE_LOGGING}"
        }
      },
      "messaging": {
        "command": "python",
        "args": ["-m", "src.core.messaging"],
        "env": {
          "APP_ENV": "${APP_ENV}",
          "LOG_LEVEL": "${LOG_LEVEL}",
          "MESSAGING_DEBUG": "${MESSAGING_DEBUG}"
        }
      },
      "project-scanner": {
        "command": "python",
        "args": ["-m", "tools.projectscanner"],
        "env": {
          "APP_ENV": "${APP_ENV}",
          "SCANNER_VERBOSE": "${SCANNER_VERBOSE}"
        }
      },
      "debate-coordinator": {
        "command": "python",
        "args": ["-m", "tools.debate_participation_tool"],
        "env": {
          "APP_ENV": "${APP_ENV}",
          "DEBATE_DEBUG": "${DEBATE_DEBUG}"
        }
      },
      "swarm-onboarding": {
        "command": "python",
        "args": ["-m", "swarm_onboarding"],
        "env": {
          "APP_ENV": "${APP_ENV}",
          "ONBOARDING_VERBOSE": "${ONBOARDING_VERBOSE}"
        }
      },
      "performance-monitor": {
        "command": "python",
        "args": ["-m", "performance_audit"],
        "env": {
          "APP_ENV": "${APP_ENV}",
          "PERFORMANCE_DEBUG": "${PERFORMANCE_DEBUG}"
        }
      },
      "filesystem": {
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-filesystem", "D:\\Agent_Cellphone_V2_Repository"],
        "env": {
          "APP_ENV": "${APP_ENV}",
          "WORKSPACE_ROOT": "D:\\Agent_Cellphone_V2_Repository"
        }
      },
  
      "pieces-memory": {
        "command": "npx",
        "args": ["-y", "@pieces-app/mcp-server"],
        "env": {
          "PIECES_API_KEY": "${PIECES_API_KEY}"
        }
      },
      "prometheus": {
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-prometheus"],
        "env": {
          "PROMETHEUS_URL": "${PROMETHEUS_URL}"
        }
      },
      "slack": {
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-slack"],
        "env": {
          "SLACK_BOT_TOKEN": "${SLACK_BOT_TOKEN}"
        }
      },
      "discord": {
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-discord"],
        "env": {
          "DISCORD_BOT_TOKEN": "${DISCORD_BOT_TOKEN}"
        }
      },
      "sqlite": {
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-sqlite", "--db-path", "D:\\Agent_Cellphone_V2_Repository\\data\\unified.db"],
        "env": {
          "APP_ENV": "${APP_ENV}",
          "DB_PATH": "D:\\Agent_Cellphone_V2_Repository\\data\\unified.db"
        }
      },
      "memory": {
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-memory"],
        "env": {
          "APP_ENV": "${APP_ENV}",
          "MEMORY_STORE_PATH": "D:\\Agent_Cellphone_V2_Repository\\runtime\\memory"
        }
      },
      "git": {
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-git", "--repository", "D:\\Agent_Cellphone_V2_Repository"],
        "env": {
          "APP_ENV": "${APP_ENV}",
          "GIT_REPOSITORY": "D:\\Agent_Cellphone_V2_Repository"
        }
      },
      "github": {
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-github"],
        "env": {
          "GITHUB_TOKEN": "${GITHUB_TOKEN}",
          "GITHUB_REPOSITORY": "Agent_Cellphone_V2_Repository"
        }
      },
      "web-search": {
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-web-search"],
        "env": {
          "APP_ENV": "${APP_ENV}",
          "SEARCH_API_KEY": "${SEARCH_API_KEY}"
        }
      }
    }
  }
  