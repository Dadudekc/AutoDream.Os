{
  "enforcement_level": "strict",
  "mandatory_operations": [
    "task_completion",
    "milestone_achievement",
    "error_occurrence",
    "system_status_change",
    "coordination_event",
    "v2_compliance_update",
    "contract_assignment",
    "cross_agent_communication",
    "message_delivery",
    "inbox_message_delivery",
    "coordinate_validation",
    "devlog_enforcement_check"
  ],
  "violation_threshold": 3,
  "auto_devlog_fallback": true,
  "required_categories": [
    "general",
    "progress", 
    "issue",
    "success",
    "warning",
    "info"
  ],
  "enforcement_actions": {
    "warning_threshold": 1,
    "blocking_threshold": 5,
    "auto_correction": true,
    "notification_webhook": null
  },
  "compliance_requirements": {
    "minimum_entries_per_day": 3,
    "required_operations_logged": [
      "task_completion",
      "milestone_achievement",
      "error_occurrence"
    ],
    "quality_standards": {
      "minimum_title_length": 10,
      "minimum_content_length": 20,
      "require_category": true
    }
  }
}
