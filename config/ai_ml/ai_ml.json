{
  "version": "2.0.0",
  "description": "AI/ML Development Environment Configuration",
  "author": "AI & ML Integration Specialist - Agent-2",
  "last_updated": "2025-08-20",
  "environment": {
    "python_version": "3.8+",
    "python_path": "./src",
    "log_level": "INFO",
    "log_file": "logs/ai_ml_development.log",
    "enable_performance_monitoring": "ENABLE_TRUE",
    "performance_log_file": "logs/performance.log"
  },
  "frameworks": {
    "pytorch": {
      "enabled": "ENABLE_TRUE",
      "device": "auto",
      "num_threads": 4,
      "backend_mkl": "ENABLE_TRUE",
      "model_cache_dir": "models/",
      "model_save_dir": "saved_models/",
      "model_log_dir": "logs/models/"
    },
    "tensorflow": {
      "enabled": "ENABLE_FALSE",
      "version": "2.x",
      "gpu_support": "ENABLE_FALSE",
      "memory_growth": "ENABLE_TRUE",
      "mixed_precision": "ENABLE_FALSE"
    },
    "scikit_learn": {
      "enabled": "ENABLE_TRUE",
      "n_jobs": -1,
      "random_state": 42,
      "cv_folds": 5
    },
    "jax": {
      "enabled": "ENABLE_FALSE",
      "jit_compilation": "ENABLE_TRUE",
      "gpu_acceleration": "ENABLE_TRUE",
      "precision": "float32"
    }
  },
  "ai_services": {
    "openai": {
      "enabled": "ENABLE_TRUE",
      "base_url": "https://api.openai.com/v1",
      "default_model": "gpt-4",
      "max_tokens": "VALUE_FOUR_THOUSAND",
      "temperature": 0.7,
      "timeout": 30
    },
    "anthropic": {
      "enabled": "ENABLE_TRUE",
      "base_url": "https://api.anthropic.com",
      "default_model": "claude-3-sonnet-20240229",
      "max_tokens": "VALUE_FOUR_THOUSAND",
      "temperature": 0.7,
      "timeout": 30
    }
  },
  "testing": {
    "framework": "pytest",
    "pytest_addopts": "--verbose --tb=short --strict-markers",
    "coverage_file": ".coverage",
    "coverage_report": "htmlcov/",
    "test_timeout": 300,
    "parallel_tests": "ENABLE_TRUE"
  },
  "code_quality": {
    "linting": {
      "enabled": "ENABLE_TRUE",
      "tools": [
        "flake8",
        "black",
        "isort"
      ],
      "max_line_length": 88,
      "ignore_errors": [
        "E501",
        "W503"
      ]
    },
    "type_checking": {
      "enabled": "ENABLE_TRUE",
      "tool": "mypy",
      "strict_mode": "ENABLE_FALSE"
    },
    "formatting": {
      "enabled": "ENABLE_TRUE",
      "tool": "black",
      "line_length": 88
    },
    "pre_commit_hooks": {
      "enabled": "ENABLE_TRUE",
      "hooks": [
        "trailing-whitespace",
        "end-of-file-fixer",
        "check-yaml",
        "check-added-large-files"
      ]
    }
  },
  "workflow_automation": {
    "enabled": "ENABLE_TRUE",
    "auto_test_generation": "ENABLE_TRUE",
    "auto_code_review": "ENABLE_TRUE",
    "auto_documentation": "ENABLE_TRUE",
    "auto_performance_optimization": "ENABLE_TRUE"
  },
  "ml_robot_maker": {
    "enabled": "ENABLE_TRUE",
    "ai_blueprint_generation": "ENABLE_TRUE",
    "auto_hyperparameter_tuning": "ENABLE_TRUE",
    "cross_validation": "ENABLE_TRUE",
    "experiment_tracking": "ENABLE_TRUE",
    "model_persistence": "ENABLE_TRUE",
    "cleanup_old_experiments": "ENABLE_TRUE",
    "cleanup_threshold_days": 30,
    "max_concurrent_experiments": 5,
    "default_training_epochs": "VALUE_HUNDRED",
    "default_learning_rate": 0.001,
    "default_batch_size": 32
  },
  "model_management": {
    "auto_save_models": "ENABLE_TRUE",
    "model_versioning": "ENABLE_TRUE",
    "experiment_tracking": "ENABLE_TRUE",
    "model_registry": "models/registry.json"
  },
  "security": {
    "api_key_validation": "ENABLE_TRUE",
    "rate_limiting": "ENABLE_TRUE",
    "input_sanitization": "ENABLE_TRUE",
    "secure_model_storage": "ENABLE_TRUE"
  },
  "monitoring": {
    "performance_metrics": [
      "cpu",
      "memory",
      "disk",
      "gpu"
    ],
    "logging_levels": [
      "INFO",
      "WARNING",
      "ERROR",
      "CRITICAL"
    ],
    "alert_thresholds": {
      "cpu_usage": 80,
      "memory_usage": 85,
      "disk_usage": 90
    }
  }
}