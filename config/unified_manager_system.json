{
  "unified_manager_system": {
    "version": "2.0.0",
    "enabled": true,
    "health_check_interval": 300,
    "auto_recovery": true,
    "max_recovery_attempts": 3,
    "startup_timeout": 60,
    "shutdown_timeout": 30,
    
    "categories": {
      "core": {
        "enabled": true,
        "priority": "critical",
        "startup_order": [
          "system_manager",
          "config_manager",
          "status_manager",
          "task_manager",
          "data_manager",
          "communication_manager",
          "health_manager"
        ]
      },
      "extended": {
        "enabled": true,
        "priority": "high",
        "startup_order": [
          "ai_manager",
          "model_manager",
          "api_key_manager",
          "ai_agent_manager",
          "workflow_manager",
          "reporting_manager",
          "dev_workflow_manager",
          "portfolio_manager",
          "risk_manager"
        ]
      },
      "specialized": {
        "enabled": true,
        "priority": "medium",
        "startup_order": [
          "performance_alert_manager",
          "autonomous_task_manager",
          "autonomous_workflow_manager",
          "autonomous_reporting_manager"
        ]
      }
    },
    
    "health_monitoring": {
      "enabled": true,
      "check_interval": 300,
      "timeout": 10,
      "failure_threshold": 3,
      "auto_restart": true,
      "health_score_threshold": 80
    },
    
    "performance": {
      "metrics_collection": true,
      "performance_tracking": true,
      "resource_monitoring": true,
      "alerting": true
    },
    
    "logging": {
      "level": "INFO",
      "format": "json",
      "output": "file",
      "file_path": "logs/unified_manager_system.log",
      "max_file_size": "10MB",
      "backup_count": 5
    },
    
    "dependencies": {
      "strict_mode": true,
      "circular_dependency_check": true,
      "dependency_timeout": 30,
      "retry_failed_dependencies": true
    }
  }
}
