{
  "warning_management": {
    "suppressed_warnings": [
      {
        "category": "DeprecationWarning",
        "patterns": [
          ".*unclosed file.*",
          ".*inspect\\.getargspec.*"
        ],
        "reason": "Known issues that are handled by the system"
      },
      {
        "category": "UserWarning",
        "patterns": [
          "matplotlib.*"
        ],
        "reason": "Matplotlib display warnings in headless environments"
      },
      {
        "category": "PendingDeprecationWarning",
        "patterns": [
          ".*"
        ],
        "reason": "Temporary suppression during transition period"
      }
    ],
    "warning_thresholds": {
      "max_warnings_per_category": 10,
      "max_warnings_per_module": 5,
      "stability_alert_threshold": 20
    }
  },
  "stability_improvements": {
    "retry_mechanisms": {
      "max_retries": "VALUE_THREE",
      "retry_delay": "SECONDS_ONE",
      "exponential_backoff": "ENABLE_TRUE"
    },
    "import_validation": {
      "validate_on_startup": "ENABLE_TRUE",
      "check_optional_modules": "ENABLE_TRUE",
      "fallback_gracefully": "ENABLE_TRUE"
    },
    "error_handling": {
      "log_all_errors": "ENABLE_TRUE",
      "suppress_known_errors": "ENABLE_FALSE",
      "max_error_log_size": "VALUE_THOUSAND"
    }
  },
  "logging": {
    "warning_level": "WARNING",
    "log_warning_counts": "ENABLE_TRUE",
    "stability_reporting": "ENABLE_TRUE"
  }
}