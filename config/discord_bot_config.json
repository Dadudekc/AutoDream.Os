{
  "description": "V2_SWARM Discord Agent Bot Configuration",
  "version": "1.0.0",
  "bot_settings": {
    "command_prefix": "!",
    "command_timeout": 300,
    "max_concurrent_commands": 10,
    "response_timeout": 600
  },
  "permissions": {
    "allowed_channels": [],
    "admin_users": [],
    "restricted_commands": []
  },
  "agent_settings": {
    "max_prompt_length": 2000,
    "max_swarm_message_length": 1000,
    "response_tracking": true,
    "command_history_limit": 100
  },
  "logging": {
    "enable_command_logging": true,
    "enable_response_logging": true,
    "log_file": "logs/discord_bot.log",
    "log_level": "INFO"
  },
  "features": {
    "enable_prompt_commands": true,
    "enable_status_commands": true,
    "enable_swarm_commands": true,
    "enable_help_commands": true,
    "enable_admin_commands": false
  },
  "integrations": {
    "webhook_notifications": true,
    "agent_response_tracking": true,
    "status_updates": true
  }
}
