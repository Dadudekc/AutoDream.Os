{
  "dashboard_id": "v2_system_health_dashboard",
  "dashboard_name": "V2 System Health Dashboard",
  "dashboard_version": "2.0.0",
  "dashboard_type": "system_monitoring",
  "description": "Real-time system health monitoring dashboard for Agent Cellphone V2",
  "status": "active",
  "priority": "critical",
  "dashboard_config": {
    "refresh_interval_seconds": 30,
    "auto_refresh": true,
    "max_data_points": 1000,
    "alert_integration": true,
    "export_enabled": true,
    "customization_allowed": true
  },
  "dashboard_layout": {
    "grid_columns": 12,
    "grid_rows": 8,
    "widgets": [
      {
        "widget_id": "system_overview",
        "widget_name": "System Overview",
        "widget_type": "summary",
        "position": {"x": 0, "y": 0, "width": 12, "height": 2},
        "data_source": "system_health_aggregator",
        "refresh_interval_seconds": 60
      },
      {
        "widget_id": "agent_status",
        "widget_name": "Agent Status",
        "widget_type": "status_grid",
        "position": {"x": 0, "y": 2, "width": 6, "height": 3},
        "data_source": "agent_monitor",
        "refresh_interval_seconds": 30
      },
      {
        "widget_id": "performance_metrics",
        "widget_name": "Performance Metrics",
        "widget_type": "line_chart",
        "position": {"x": 6, "y": 2, "width": 6, "height": 3},
        "data_source": "performance_monitor",
        "refresh_interval_seconds": 15
      },
      {
        "widget_id": "workflow_status",
        "widget_name": "Workflow Status",
        "widget_type": "workflow_monitor",
        "position": {"x": 0, "y": 5, "width": 8, "height": 3},
        "data_source": "workflow_monitor",
        "refresh_interval_seconds": 45
      },
      {
        "widget_id": "alert_panel",
        "widget_name": "Active Alerts",
        "widget_type": "alert_list",
        "position": {"x": 8, "y": 5, "width": 4, "height": 3},
        "data_source": "alert_manager",
        "refresh_interval_seconds": 10
      }
    ]
  },
  "monitoring_metrics": {
    "system_health": [
      {
        "metric_id": "overall_health_score",
        "metric_name": "Overall Health Score",
        "metric_type": "percentage",
        "threshold_warning": 80,
        "threshold_critical": 60,
        "calculation": "weighted_average",
        "weight_factors": {
          "agent_health": 0.3,
          "workflow_health": 0.25,
          "performance_health": 0.25,
          "resource_health": 0.2
        }
      },
      {
        "metric_id": "system_availability",
        "metric_name": "System Availability",
        "metric_type": "percentage",
        "threshold_warning": 99.5,
        "threshold_critical": 99.0,
        "calculation": "uptime_percentage",
        "time_window_hours": 24
      }
    ],
    "agent_metrics": [
      {
        "metric_id": "active_agents",
        "metric_name": "Active Agents",
        "metric_type": "count",
        "threshold_warning": 6,
        "threshold_critical": 4
      },
      {
        "metric_id": "agent_response_time",
        "metric_name": "Average Response Time",
        "metric_type": "milliseconds",
        "threshold_warning": 2000,
        "threshold_critical": 5000
      },
      {
        "metric_id": "agent_task_completion_rate",
        "metric_name": "Task Completion Rate",
        "metric_type": "percentage",
        "threshold_warning": 90,
        "threshold_critical": 75
      }
    ],
    "workflow_metrics": [
      {
        "metric_id": "active_workflows",
        "metric_name": "Active Workflows",
        "metric_type": "count",
        "threshold_warning": 80,
        "threshold_critical": 95
      },
      {
        "metric_id": "workflow_success_rate",
        "metric_name": "Workflow Success Rate",
        "metric_type": "percentage",
        "threshold_warning": 95,
        "threshold_critical": 85
      },
      {
        "metric_id": "average_workflow_duration",
        "metric_name": "Average Workflow Duration",
        "metric_type": "minutes",
        "threshold_warning": 60,
        "threshold_critical": 120
      }
    ],
    "performance_metrics": [
      {
        "metric_id": "cpu_utilization",
        "metric_name": "CPU Utilization",
        "metric_type": "percentage",
        "threshold_warning": 70,
        "threshold_critical": 85
      },
      {
        "metric_id": "memory_utilization",
        "metric_name": "Memory Utilization",
        "metric_type": "percentage",
        "threshold_warning": 75,
        "threshold_critical": 90
      },
      {
        "metric_id": "network_throughput",
        "metric_name": "Network Throughput",
        "metric_type": "megabytes_per_second",
        "threshold_warning": 100,
        "threshold_critical": 150
      }
    ]
  },
  "alert_configuration": {
    "alert_levels": [
      {
        "level": "info",
        "color": "#17a2b8",
        "notification": "log_only"
      },
      {
        "level": "warning",
        "color": "#ffc107",
        "notification": "dashboard_highlight"
      },
      {
        "level": "critical",
        "color": "#dc3545",
        "notification": "immediate_alert"
      }
    ],
    "notification_channels": [
      "dashboard",
      "email",
      "slack",
      "sms"
    ],
    "escalation_rules": [
      {
        "condition": "critical_alert_duration_minutes > 15",
        "action": "escalate_to_team_lead"
      },
      {
        "condition": "system_health_score < 50",
        "action": "escalate_to_system_admin"
      }
    ]
  },
  "data_sources": {
    "system_health_aggregator": {
      "type": "aggregator",
      "source": "multiple_metrics",
      "update_frequency_seconds": 60
    },
    "agent_monitor": {
      "type": "real_time",
      "source": "agent_status_service",
      "update_frequency_seconds": 30
    },
    "performance_monitor": {
      "type": "real_time",
      "source": "performance_metrics_service",
      "update_frequency_seconds": 15
    },
    "workflow_monitor": {
      "type": "real_time",
      "source": "workflow_engine",
      "update_frequency_seconds": 45
    },
    "alert_manager": {
      "type": "event_driven",
      "source": "alert_service",
      "update_frequency_seconds": 10
    }
  },
  "export_formats": [
    "json",
    "csv",
    "pdf",
    "excel"
  ],
  "access_control": {
    "view_permissions": ["monitor", "analyst", "admin"],
    "edit_permissions": ["admin"],
    "export_permissions": ["analyst", "admin"]
  },
  "metadata": {
    "created_by": "V2_SYSTEM",
    "created_date": "2025-08-23T19:30:00Z",
    "last_modified": "2025-08-23T19:30:00Z",
    "dashboard_category": "system_monitoring",
    "compatibility": ["v2_monitoring_system", "v2_alert_manager", "v2_performance_monitor"]
  }
}
